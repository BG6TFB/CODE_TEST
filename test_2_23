#include<stdio.h>
#include<ctype.h>
int main()
{
	int i=0;
	char arr[]="I Am A StuDENT";
	while(arr[i])
	{
		if(isupper(arr[i]))
		{
			arr[i]=tolower(arr[i]);
		}
		i++;
	}
	printf("%s\n",arr);
	return 0;
}
